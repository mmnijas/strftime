  "license": "MIT",
  "type": "library",
  "support": {
        "issues": "https://github.com/mmnijas/strftime/issues",
        "source": "https://github.com/mmnijas/strftime"
    },
  "autoload": {
    "psr-4": {
      "MmNijas\\Strftime\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1"
  },
  "authors": [
    {
      "name": "Nijas M M",
      "email": "mmnijas@gmail.com",
      "homepage": "https://mmnijas.in/"
    }
